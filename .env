# Multi-tenant LLM Router Environment Configuration

# Server configuration
PORT=8080
ADMIN_TOKEN=admin123
ROUTER_POLICY=cheapest

# Enable multi-tenant features with fallback JSON file
TENANTS_JSON=tenants.json
ENABLE_USAGE_TRACKING=true

# Mock provider for testing (no real API keys needed)
ENABLE_MOCK_PROVIDER=1
MOCK_MEAN_LATENCY_MS=40
MOCK_P95_LATENCY_MS=120
MOCK_ERROR_RATE=0.01
MOCK_COST_PER_1K_TOKENS_USD=0.002

# Provider configurations
OPENAI_MODEL=gpt-4o
BEDROCK_MODEL_ID=anthropic.claude-3-haiku
BEDROCK_REGION=us-east-1

# Canary configuration
CANARY_STAGES=1,5,25
CANARY_WINDOW=200
CANARY_BURN_MULTIPLIER=2